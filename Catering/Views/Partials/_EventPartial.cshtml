@model EventPage

<div id="@Model.Id" class="event-container">
    <div class="accordian fusion-accordian">
        <div class="event-content">
            @Model.GetGridHtml(Html, "pageBody")
        </div>
        @if (Model.Menus.Any())
        {
            foreach (var menu in Model.Menus)
            {                               
                @Html.Partial("~/Views/Partials/_MenuPartial.cshtml", (Menu)menu)
            }
        }
    </div>
</div>