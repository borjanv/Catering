@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Gallery>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}
@section header{
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.0.47/jquery.fancybox.min.css" />
}
@section scripts
{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.0.47/jquery.fancybox.min.js"></script>
}
<div id="sliders-container">
</div>
<div id="main" class="clearfix " style="">
    <div class="avada-row" style="">
        <div id="content" class="full-width">
            <div id="post-@Model.Content.Id" class="post-@Model.Content.Id page type-page status-publish hentry">
                <div class="post-content">
                    <div class="fusion-one-fourth one_fourth fusion-column"></div>
                    <h1>@(!string.IsNullOrEmpty(Model.Content.PageTitle) ? Model.Content.PageTitle : Model.Content.Name)</h1>
                    @Umbraco.RenderMacro("GalleryMacro")
                </div>
            </div>
        </div>
    </div>
</div>
