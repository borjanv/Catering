<script type="text/javascript">
    jQuery('#open').collapsible('accordion-open', {
        contentOpen: 1
    });
    jQuery(".no-link").click(function(e) {
        e.preventDefault();
    });
    jQuery(document).ready(function () {
        jQuery('.tabs').tabslet();
    });
    

    // global vars
    gg_galleria_toggle_info = false;
    gg_galleria_fx = 'fadeslide';
